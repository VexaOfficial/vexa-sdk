# Solana RPC URL - defaults to devnet if not set
SOLANA_RPC_URL=https://api.devnet.solana.com

# Private key for wallet automation - NEVER commit this!
# Can be base58 encoded string or JSON array of numbers
# Example: PRIVATE_KEY=[174,47,154,16,...] or PRIVATE_KEY=base58string
PRIVATE_KEY=

# Cold wallet address for auto-send actions
COLD_WALLET=

# Optional: Devnet integration test flag
DEVNET_INTEGRATION=false